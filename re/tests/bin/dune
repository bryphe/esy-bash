(executable
  (name test_runner)
  (modules test_runner)
  (libraries EsyBashLibTests ppx_inline_test.runner.lib))

(alias
  (name    runtest)
  (deps    test_runner.exe)
  (action  (run %{deps} inline-test-runner EsyBashLibTests)))




